# 生产环境变量配置

# 后端服务配置
BACKEND_PORT=8080
BACKEND_HOST=0.0.0.0
DATABASE_PATH=/data/db/spatial_data.db
OUTPUT_DIR=/data/output
LOG_LEVEL=2

# 前端服务配置
VITE_API_BASE_URL=/api
VITE_CACHE_STRATEGY=aggressive
VITE_DEVELOPMENT_MODE=false

# Docker 配置
COMPOSE_PROJECT_NAME=leaf-prod

# 安全配置（示例）
# SSL_CERT_PATH=/etc/ssl/certs/leaf.crt
# SSL_KEY_PATH=/etc/ssl/private/leaf.key

# 资源限制
BACKEND_MEMORY_LIMIT=1G
BACKEND_CPU_LIMIT=1.0