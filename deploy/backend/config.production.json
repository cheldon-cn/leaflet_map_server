{
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "worker_threads": 4,
    "max_request_size": 10485760,
    "timeout_seconds": 30
  },
  "database": {
    "path": "/data/db/spatial_data.db",
    "connection_pool_size": 5
  },
  "cache": {
    "memory_max_items": 100,
    "disk_cache_dir": "/data/cache",
    "ttl_seconds": 30
  },
  "limits": {
    "max_image_width": 4096,
    "max_image_height": 4096,
    "max_features_per_request": 10000
  },
  "output_dir": "/data/output",
  "log": {
    "file": "/data/logs/map_server.log",
    "level": 2
  }
}